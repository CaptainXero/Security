<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <include layout="@layout/titlebar"/>
    <EditText
        android:id="@+id/et_numlocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:inputType="phone"
        android:drawableLeft="@drawable/numbelongtoicon"
        android:background="@drawable/numbelongto_et_bg"
        android:drawablePadding="3dp"
        android:hint="请输入要查询的号码"/>
    <Button
        android:id="@+id/bt_numlocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/btn_numaddressbg_selector"/>
    <TextView
        android:id="@+id/tv_numlocation_result"
        style="@style/textview16sp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:textColor="#696969"/>

</LinearLayout>
